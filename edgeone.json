{
  "redirects": [
    {
      "source": "/",
      "destination": "/reports/",
      "permanent": false
    },
    {
      "source": "/reports",
      "destination": "/reports/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/reports/(.*)",
      "destination": "/reports/$1"
    }
  ],
  "headers": [
    {
      "source": "/**/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/**/*.{png,jpg,jpeg,gif,ico,svg}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    }
  ],
  "trailingSlash": true,
  "cleanUrls": true
}